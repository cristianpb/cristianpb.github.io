{
  "name": "cristianpb.github.io",
  "version": "1.0.0",
  "description": "With Jekyll and Github Pages",
  "main": "gulpfile.js",
  "scripts": {
    "build": "bundle exec jekyll build --config _config.yml,_config_algolia.yml",
    "serve": "bundle exec jekyll serve --livereload --config _config.yml,_config_algolia.yml",
    "algolia": "bundle exec jekyll algolia --config _config.yml,_config_algolia.yml",
    "css:build": "tailwindcss -i _sass/main.scss -o _includes/main.css",
    "watch": "tailwindcss -i _sass/main.scss -o _includes/main.css --watch",
    "images": "gulp images",
    "thumbnails": "gulp thumbnails"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristianpb/cristianpb.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cristianpb/cristianpb.github.io/issues"
  },
  "homepage": "https://github.com/cristianpb/cristianpb.github.io#readme",
  "devDependencies": {
    "bulma": "^0.9.4",
    "daisyui": "^3.5.0",
    "tailwindcss": "^3.3.3"
  }
}
